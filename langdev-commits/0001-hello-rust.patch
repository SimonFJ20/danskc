From 3217051e7fde9619d0370a0ea10ce6a75c128070 Mon Sep 17 00:00:00 2001
From: SimonFJ20 <simonfromjakobsen@gmail.com>
Date: Sat, 22 Oct 2022 00:23:55 +0200
Subject: [PATCH] hello rust

---
 danskc/runtime/.gitignore  | 3 +--
 danskc/runtime/Cargo.lock  | 7 +++++++
 danskc/runtime/Cargo.toml  | 8 ++++++++
 danskc/runtime/src/main.rs | 3 +++
 4 files changed, 19 insertions(+), 2 deletions(-)
 create mode 100644 danskc/runtime/Cargo.lock
 create mode 100644 danskc/runtime/Cargo.toml
 create mode 100644 danskc/runtime/src/main.rs

diff --git a/danskc/runtime/.gitignore b/danskc/runtime/.gitignore
index 1b51c15..9f97022 100644
--- a/danskc/runtime/.gitignore
+++ b/danskc/runtime/.gitignore
@@ -1,2 +1 @@
-danskr
-*.o
\ No newline at end of file
+target/
\ No newline at end of file
diff --git a/danskc/runtime/Cargo.lock b/danskc/runtime/Cargo.lock
new file mode 100644
index 0000000..0728315
--- /dev/null
+++ b/danskc/runtime/Cargo.lock
@@ -0,0 +1,7 @@
+# This file is automatically @generated by Cargo.
+# It is not intended for manual editing.
+version = 3
+
+[[package]]
+name = "runtime"
+version = "0.1.0"
diff --git a/danskc/runtime/Cargo.toml b/danskc/runtime/Cargo.toml
new file mode 100644
index 0000000..65e9c1d
--- /dev/null
+++ b/danskc/runtime/Cargo.toml
@@ -0,0 +1,8 @@
+[package]
+name = "runtime"
+version = "0.1.0"
+edition = "2021"
+
+# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html
+
+[dependencies]
diff --git a/danskc/runtime/src/main.rs b/danskc/runtime/src/main.rs
new file mode 100644
index 0000000..e7a11a9
--- /dev/null
+++ b/danskc/runtime/src/main.rs
@@ -0,0 +1,3 @@
+fn main() {
+    println!("Hello, world!");
+}
